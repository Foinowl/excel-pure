{
    "parser": "babel-eslint",
    "env": {
        "es6":true,
        "browser":true,
        "node":true,
        "jest/globals":true
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "linebreak-style": "off",
        "operator-linebreak":"off",
        "require-jsdoc":"off",
        "comma-dangle":"off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}